{
  "presets": [
    [
      '@babel/preset-env',
      {
        useBuiltIns: 'usage',
        debug: false,
        corejs: 3,
        modules: false,
        loose: true,
      }
    ],
    ['@babel/preset-react'],
  ],
  plugins: [
    '@babel/plugin-transform-runtime',
    '@babel/plugin-syntax-dynamic-import',
    '@babel/plugin-proposal-class-properties',
    'lodash',
  ]
}
